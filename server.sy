use game

start :: fn {
    game.init'
    l_bind_quit("quit")

    n_rpc_start_server'
    loop !l_input_down("quit") {
        n_rpc_resolve'

        l_update'
        l_render'

        game.process_input' l_delta'
        game.update' l_delta'
        game.draw'

        n_rpc_clients' game.set_entities, game.entities, game.players
    }
}
